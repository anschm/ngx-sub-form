    "cypress": "12.0.2",
    "cz-conventional-changelog": "3.3.0",
    "embedme": "1.22.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
